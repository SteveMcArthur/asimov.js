{
  "exportScripts": [
    "core/Base"
  ],
  "dependencies": {
    "production": {
      "express": "4.0.0",
      "express-livereload": "0.0.24",
      "require-uncache": "0.0.2",
      "static-asset": "0.5.2",
      "yaml-js": "0.0.8",
      "compression": "1.0.1",
      "marked": "0.3.1",
      "uglify-js": "2.4.13",
      "stylus": "0.43.1",
      "nib": "1.0.2",
      "browserify": "3.40.0",
      "watchdirectory": "0.1.5"
    },
    "test": {
      "mocha": "1.18.2",
      "zombie": "2.0.0-alpha29",
      "chai": "1.9.0",
      "sinon": "1.9.0",
      "sinon-chai": "2.5.0"
    },
    "coverage": {
      "istanbul": "0.2.7"
    },
    "complexity": {
      "plato": "1.0.1"
    }
  },
  "paths": {
    "outputDir": "build",
    "frameworkPages": "{{framework}}/lib/pages",
    "applications": "public/applications",
    "configs": "environments",
    "styles": [
      "site/styles",
      "{{framework}}/site/styles"
    ],
    "scripts": [
      "lib",
      "{{framework}}/lib"
    ],
    "content": "content",
    "data": "site/data",
    "templates": [
      "site/templates",
      "{{framework}}/site/templates"
    ],
    "helpers": [
      "{{framework}}/lib/helpers",
      "lib/helpers"
    ],
    "middleware": [
      "lib/middleware",
      "{{framework}}/lib/middleware"
    ],
    "initializers": [
      "{{framework}}/lib/initializers",
      "lib/initializers"
    ],
    "commands": [
      "lib/commands",
      "{{framework}}/lib/commands"
    ]
  },
  "localization": {
    "defaultLangCode": "en"
  },
  "server": {
    "logDelay": 15,
    "workerLogInterval": 5
  },
  "attributes": {
    "middleware": {
      "page": [
        "url",
        "alias",
        "redirect",
        "middleware",
        "title"
      ]
    }
  },
  "middleware": [],
  "initializers": []
}